# Token du bot Telegram (obtenu depuis @BotFather)
BOT_TOKEN=your_bot_token_here

# ID Telegram de l'administrateur principal (obtenu via @userinfobot)
ADMIN_ID=your_telegram_id_here

# URI MongoDB (optionnel - utilise la valeur par défaut si non spécifié)
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/dbname

# URL de webhook (optionnel, pour le mode webhook)
WEBHOOK_URL=

# Port pour le serveur webhook (optionnel)
PORT=3000